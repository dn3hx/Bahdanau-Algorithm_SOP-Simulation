      "source": [
        "import torch\n",
        "import torch.nn as nn\n",
        "import torch.nn.functional as F\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "torch.manual_seed(0)\n",
        "\n",
        "# -----------------------------\n",
        "# Simulated Multi-Turn Setup\n",
        "# -----------------------------\n",
        "\n",
        "turns = 3\n",
        "hidden_size = 8\n",
        "metadata_size = 3\n",
        "\n",
        "# Simulated hidden states for 3 turns\n",
        "encoder_hidden = torch.randn(turns, hidden_size)\n",
        "\n",
        "# Decoder state (current context)\n",
        "decoder_state = torch.randn(hidden_size)\n",
        "\n",
        "# Simulated escalation metadata\n",
        "# [prior_warnings, rapid_reply, repeated_user_attack]\n",
        "metadata_vectors = torch.tensor([\n",
        "    [0.0, 0.0, 0.0],   # Turn 1 - neutral\n",
        "    [1.0, 1.0, 1.0],   # Turn 2 - moderate escalation\n",
        "    [3.0, 1.0, 1.0]    # Turn 3 - strong escalation\n",
        "])\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Standard Attention\n",
        "# -----------------------------\n",
        "\n",
        "class StandardAttention(nn.Module):\n",
        "    def __init__(self, hidden_size):\n",
        "        super().__init__()\n",
        "        self.W_s = nn.Linear(hidden_size, hidden_size)\n",
        "        self.W_h = nn.Linear(hidden_size, hidden_size)\n",
        "        self.v = nn.Linear(hidden_size, 1)\n",
        "\n",
        "    def forward(self, decoder_state, encoder_hidden):\n",
        "        scores = []\n",
        "        for h_i in encoder_hidden:\n",
        "            score = self.v(torch.tanh(self.W_s(decoder_state) + self.W_h(h_i)))\n",
        "            scores.append(score)\n",
        "        scores = torch.stack(scores).squeeze()\n",
        "        return F.softmax(scores, dim=0)\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Metadata-Enhanced Attention\n",
        "# -----------------------------\n",
        "\n",
        "class EnhancedAttention(nn.Module):\n",
        "    def __init__(self, hidden_size, metadata_size):\n",
        "        super().__init__()\n",
        "        self.W_s = nn.Linear(hidden_size, hidden_size)\n",
        "        self.W_h = nn.Linear(hidden_size, hidden_size)\n",
        "        self.W_m = nn.Linear(metadata_size, hidden_size)\n",
        "        self.v = nn.Linear(hidden_size, 1)\n",
        "\n",
        "    def forward(self, decoder_state, encoder_hidden, metadata_vectors):\n",
        "        scores = []\n",
        "        for i, h_i in enumerate(encoder_hidden):\n",
        "            score = self.v(torch.tanh(\n",
        "                self.W_s(decoder_state) +\n",
        "                self.W_h(h_i) +\n",
        "                self.W_m(metadata_vectors[i])\n",
        "            ))\n",
        "            scores.append(score)\n",
        "        scores = torch.stack(scores).squeeze()\n",
        "        return F.softmax(scores, dim=0)\n",
        "\n",
        "\n",
        "# -----------------------------\n",
        "# Run Simulation\n",
        "# -----------------------------\n",
        "\n",
        "standard_model = StandardAttention(hidden_size)\n",
        "enhanced_model = EnhancedAttention(hidden_size, metadata_size)\n",
        "\n",
        "standard_attention = standard_model(decoder_state, encoder_hidden).detach().numpy()\n",
        "enhanced_attention = enhanced_model(decoder_state, encoder_hidden, metadata_vectors).detach().numpy()\n",
        "\n",
        "print(\"Standard Attention:\", standard_attention)\n",
        "print(\"Enhanced Attention:\", enhanced_attention)\n",
        "\n",
        "# -----------------------------\n",
        "# Visualization\n",
        "# -----------------------------\n",
        "\n",
        "turn_labels = [\"Turn 1\", \"Turn 2\", \"Turn 3\"]\n",
        "x = np.arange(len(turn_labels))\n",
        "width = 0.35\n",
        "\n",
        "plt.figure(figsize=(8,5))\n",
        "plt.bar(x - width/2, standard_attention, width, label='Standard')\n",
        "plt.bar(x + width/2, enhanced_attention, width, label='Enhanced')\n",
        "plt.xticks(x, turn_labels)\n",
        "plt.ylabel(\"Attention Weight\")\n",
        "plt.title(\"Multi-Turn Escalation Modeling Comparison\")\n",
        "plt.legend()\n",
        "plt.show()\n",
        "\n",
        "# Difference Plot\n",
        "difference = enhanced_attention - standard_attention\n",
        "\n",
        "plt.figure(figsize=(8,5))\n",
        "plt.bar(turn_labels, difference)\n",
        "plt.axhline(0)\n",
        "plt.ylabel(\"Attention Shift\")\n",
        "plt.title(\"Escalation Sensitivity After Metadata Injection\")\n",
        "plt.show()\n"
      ]
    }
  ]
}